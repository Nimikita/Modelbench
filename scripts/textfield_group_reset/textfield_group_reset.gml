/// textfield_group_reset()
/// @desc Resets added textfields for groups

function textfield_group_reset()
{
	textfield_name = null
	textfield_value = null
	textfield_default = null
	textfield_script = null
	textfield_axis = null
	textfield_textbox = null
	textfield_icon = null
	textfield_mul = null
	textfield_min = null
	textfield_max = null
	textfield_caption = null
	textfield_amount = 0
}
